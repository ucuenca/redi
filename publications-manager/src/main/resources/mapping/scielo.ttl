    r2r:targetPattern "?SUBJ bibo:issn ?b" .


uc:publicationDoi a r2r:Mapping ;
    r2r:partOfMappingCollection uc:mapColl1 ;
    r2r:sourcePattern "?SUBJ scielo:doi ?b" ;
